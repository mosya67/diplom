<Page x:Class="diplomZ.FaultInfoEdit"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:diplomZ"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="FaultInfoEdit">

    <Grid>
        <Border BorderBrush="Black" BorderThickness="1"/>
        <Border BorderBrush="Black" BorderThickness="1"/>
        <Grid Background="#FFE5E5E5" Height="443.04" Margin="0,3,0,3">
            <Button x:Name="searchPage_button_goBack" Content="Назад" HorizontalAlignment="Left" Height="25" Margin="10,7,0,0" VerticalAlignment="Top" Width="64" Click="Button_Click"/>
            <TextBox x:Name="_FaultInfoEdit_Name" HorizontalAlignment="Left" Height="19" Margin="138,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
            <Label Content="Дата обращения" HorizontalAlignment="Left" Height="24" Margin="10,53,0,0" VerticalAlignment="Top" Width="116"/>
            <TextBox x:Name="_FaultInfoEdit_Name_Копировать" HorizontalAlignment="Left" Height="19" Margin="138,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
            <Label Content="Статус заказа" HorizontalAlignment="Left" Height="24" Margin="10,82,0,0" VerticalAlignment="Top" Width="116"/>
            <TextBox x:Name="_FaultInfoEdit_Name_Копировать1" HorizontalAlignment="Left" Height="19" Margin="138,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
            <Label Content="Имя" HorizontalAlignment="Left" Height="24" Margin="10,111,0,0" VerticalAlignment="Top" Width="116"/>
            <TextBox x:Name="_FaultInfoEdit_Name_Копировать2" HorizontalAlignment="Left" Height="19" Margin="138,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
            <Label Content="Фамилия" HorizontalAlignment="Left" Height="24" Margin="10,140,0,0" VerticalAlignment="Top" Width="116"/>
            <TextBox x:Name="_FaultInfoEdit_Name_Копировать3" HorizontalAlignment="Left" Height="19" Margin="138,174,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
            <Label Content="Отчество" HorizontalAlignment="Left" Height="24" Margin="10,169,0,0" VerticalAlignment="Top" Width="116"/>
            <TextBox x:Name="_FaultInfoEdit_Name_Копировать4" HorizontalAlignment="Left" Height="19" Margin="138,203,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
            <Label Content="Тип устройства" HorizontalAlignment="Left" Height="24" Margin="10,198,0,0" VerticalAlignment="Top" Width="116"/>
            <TextBox x:Name="_FaultInfoEdit_Name_Копировать5" HorizontalAlignment="Left" Height="19" Margin="138,232,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
            <Label Content="Модель устройства" HorizontalAlignment="Left" Height="24" Margin="10,227,0,0" VerticalAlignment="Top" Width="116"/>
            <TextBox x:Name="_FaultInfoEdit_Name_Копировать6" HorizontalAlignment="Left" Height="112" Margin="138,261,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
            <Label Content="Описание проблемы" HorizontalAlignment="Left" Height="24" Margin="10,256,0,0" VerticalAlignment="Top" Width="128"/>
        </Grid>

    </Grid>
</Page>
